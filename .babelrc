{
  "only": "*.js",
  "presets": [
    ["env", {
      "targets": {
        "node": 4
      }
    }]
  ],
  "plugins": [
    ["babel-plugin-transform-builtin-extend", { 
      "globals": ["Error", "Array", "Map"]
    }],
    ["transform-class-properties"],
    ["transform-inline-imports-commonjs"],
    ["transform-flow-strip-types"]
  ]
}